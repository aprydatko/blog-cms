{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "analyze": "cross-env BUNDLE_ANALYZE=both next build",
    "export": "cross-env NODE_ENV=production next export",
    "stylelint": "stylelint \"**/*.css\" --fix",
    "lint": "eslint ./src/** ./pages/** -c .eslintrc --ext ts,tsx,js,jsx --color",
    "format": "prettier --write 'src/**/*.{ts,tsx,scss}' 'pages/**/*.{ts,tsx,scss}'",
    "isready": "npm run format && npm run lint && npm run build"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.8.7",
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "framer-motion": "^4.1.17",
    "next": "12.1.4",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-icons": "^4.3.1"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.2.1",
    "@types/node": "17.0.23",
    "@types/react": "17.0.43",
    "@types/react-dom": "17.0.14",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "@zeit/next-sass": "1.0.1",
    "eslint": "^8.12.0",
    "eslint-config-next": "12.1.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "7.0.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.4.0",
    "node-sass": "7.0.1",
    "prettier": "2.6.2",
    "prettier-eslint": "13.0.0",
    "prettier-eslint-cli": "5.0.1",
    "sass": "^1.49.11",
    "stylelint": "^14.6.1",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-order-config-standard": "^0.1.3",
    "typescript": "4.6.3"
  }
}
